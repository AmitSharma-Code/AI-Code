    font-size: 15px;
    margin-top: 12px;
    padding: 0px;
    color: #000;
		font-family: 'Poppins' !important;
}
.portfolio_masonry_content_wrapper.gallery_grid_content_wrapper .tg_two_cols {
    width: calc(80% - 20px);
    z-index: 9;
    padding-top: 360px;
}


